<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.modelingbrain.home">
    <!--android:versionCode="345345345"
    android:versionName="5.0.0"-->

<!--
    <uses-sdk
        android:maxSdkVersion="23"
        android:minSdkVersion="11"
        android:targetSdkVersion="23" />
    -->

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:fullBackupContent="true"
        android:allowBackup="true"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".detailModel.DetailActivity"
            android:label="@string/title_activity_detail"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name=".chooseModel.ActivityChooseModel"
            android:label="@string/title_activity_detail"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name=".about.ActivityAbout"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>


        <activity
            android:name=".opensave.OpenActivity"
            android:label="@string/title_activity_open"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name=".opensave.SaveActivity"
            android:label="@string/title_activity_save"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

    </application>

</manifest>
